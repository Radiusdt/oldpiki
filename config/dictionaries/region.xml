            <null_value></null_value>
        </attribute>
        <attribute>
            <name>name_en</name>
            <type>String</type>
            <null_value></null_value>
        </attribute>
        <attribute>
            <name>country_id</name>
            <type>Int32</type>
            <null_value>0</null_value>
        </attribute>
    </structure>
